{
    "name": "csbill/csbill",
    "description": "General Billing Application",
    "type": "application",
    "keywords": [
        "billing",
        "general billing",
        "open-source billing"
    ],
    "homepage": "http://csbill.github.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre du Plessis",
            "email": "csbill@customscripts.co.za"
        }
    ],
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.4",
        "symfony/symfony": "~2.4",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.3",
        "jms/security-extra-bundle": "~1.5",
        "doctrine/doctrine-migrations-bundle": "2.1.x-dev",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "apy/datagrid-bundle": "~2",
        "zendframework/zend-stdlib": "~2.2",
        "pagerfanta/pagerfanta": "~1.0",
        "stof/doctrine-extensions-bundle": "~1.1",
        "friendsofsymfony/jsrouting-bundle": "~1.4",
        "knplabs/knp-menu-bundle": "2.0.x-dev",
        "customscripts/core-bundle": "dev-master",
        "customscripts/user-bundle": "dev-master",
        "customscripts/settings-bundle": "dev-master",
        "rhumsaa/uuid": "~2.5",
        "moontoast/math": "~1.1"
    },
    "require-dev" : {
        "jms/debugging-bundle": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "branch-alias": {
            "dev-master": "0.1.0-dev"
        }
    }
}
